<h1>Editing album_art</h1>

<%= error_messages_for :album_art %>

<% form_for(@album_art) do |f| %>
  <p>
    <b>User</b><br />
    <%= f.text_field :user_id %>
  </p>

  <p>
    <b>Album name</b><br />
    <%= f.text_field :album_name %>
  </p>

  <p>
    <b>Artist name</b><br />
    <%= f.text_field :artist_name %>
  </p>

  <p>
    <b>Image</b><br />
    <%= f.text_field :image %>
  </p>

  <p>
    <b>Image size</b><br />
    <%= f.text_field :image_size %>
  </p>

  <p>
    <b>Image filesize</b><br />
    <%= f.text_field :image_filesize %>
  </p>

  <p>
    <b>Thumbnail</b><br />
    <%= f.text_field :thumbnail %>
  </p>

  <p>
    <b>Score</b><br />
    <%= f.text_field :score %>
  </p>

  <p>
    <b>Created at</b><br />
    <%= f.datetime_select :created_at %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @album_art %> |
<%= link_to 'Back', album_arts_path %>
